@model MVC_L1.Models.EmployeeView

@{
    ViewBag.Title = "Карточка сотрудника";
}

    <div>
        <h4>Сотрудник</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
            <dd>
                @Model.LastName @Model.Name @Model.Patronymic
            </dd>
            </dt>

            <dd>
                @Model.Age
            </dd>
            <dd>
                @Model.ID
            </dd>

        </dl>
    </div>
    <div>

        @Html.ActionLink("Список работников", "Index")
    </div>
