@using MVC_L1.Models;

@model IEnumerable<EmployeeView>
@{
    ViewBag.Title = "Список сотрудников";
}

    <div class="container">
    <table class="table table-condensed">
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.LastName</td>
            <td>@item.Name</td>
            <td>@item.Patronymic</td>
            <td>@item.Age</td>
            @*<td>@Html.ActionLink("Детали", "Details", new { guid = item.ID })</td>*@
            <td><a href="@Url.Action("Details", new { guid = item.ID })">Детали</a></td>
        </tr>
        }
    </table>
    </div>
