@using MVC_L1.Models;

@model IEnumerable<EmployeeView>
@{
    ViewBag.Title = "Карточка сотрудника";
}

    <p>Привет</p>
    <table>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.LastName</td>
            <td>@item.Name</td>
            <td>@item.Patronymic</td>
            <td>@item.Age</td>
            <td>@Html.ActionLink("Детали", "Details", new { guid = item.ID })</td>
        </tr>
        }
    </table>
