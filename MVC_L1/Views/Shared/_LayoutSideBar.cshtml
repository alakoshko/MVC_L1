
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section top{
    @await RenderSectionAsync("top", false)
}

<section>
    <div class="container">
        <div class="row">
            <div class="col-sm-3">
                @await Html.PartialAsync("Partial/_LeftSideBar")
            </div>
            <div class="col-sm-9 padding-right">
                @RenderBody()
            </div>
        </div>
    </div>
</section>


